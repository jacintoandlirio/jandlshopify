          }
        },
        "column_nacLfi": {
          "type": "column",
          "settings": {
            "image": "shopify://shop_images/Mar25-Perseverance_Journal_Lilac-Personalization.png",
            "title": "Personalization",
            "text": "<p>We have no minimum order quantity for personalization using laser engraving or colored printing. MOQ applies for other personalization finishes like embroidery, glitter, hologram, vinyl etc..</p>",
            "link_label": "Choose personalization lineup",
            "link": "shopify://collections/personalization"
          }
        },
        "column_emmj4j": {
          "type": "column",
          "settings": {
