{
  "name": "t:sections.header.name",
  "type": "header",
  "sections": {
    "header": {
      "type": "header",
      "custom_css": [
        "header {font-size: 45px; height: 300px;}",
        ".gm-target {display: inline-block; position: relative; color: #0087ca;}",
        ".gm-target:hover::after {transform: scaleX(1); transform-origin: bottom left;}",
        ".gm-target::after {position: absolute; width: 100%; transform: scaleX(0); height: 2px; bottom: 0; left: 0; background-color: #0087ca; transform-origin: bottom right; transition: transform 0.25s ease-out;}"
      ],
      "settings": {
        "logo_position": "top-center",
        "menu": "main-menu",
        "menu_type_desktop": "mega",
        "sticky_header_type": "reduce-logo-size",
        "show_line_separator": true,
        "color_scheme": "scheme-1",
        "menu_color_scheme": "scheme-1",
        "enable_country_selector": true,
        "enable_language_selector": true,
        "mobile_logo_position": "center",
        "margin_bottom": 0,
        "padding_top": 4,
        "padding_bottom": 4
      }
    }
  },
  "order": [
    "header"
  ]
}
